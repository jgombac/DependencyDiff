{
  "project_name": "jira-clone-angular",
  "repository": "https://github.com/trungk18/jira-clone-angular",
  "first_commit": "40f9e3b3b897e538ebea0e26c3bd650108e73051",
  "use_hash": false,
  "min_changes": 30,
  "dockerfile": [
    "FROM node:10.13.0-alpine AS build",
    "WORKDIR /usr/src",
    "COPY frontend/package*.json ./",
    "RUN npm install",
    "COPY frontend/. .",
    "RUN npm run build",
    "FROM nginx:1.17.1-alpine",
    "COPY --from=build /usr/src/dist /usr/share/nginx/html",
    "RUN echo \"server {listen 80; location / { root /usr/share/nginx/html; index /index.html; } error_page 404 /; }\" > /etc/nginx/conf.d/default.conf"
  ],
  "wait_until_clickable": "//a",
  "skip_xpath": [],
  "forms": [
  ],
  "repeatable_elements": [],
  "clean_text_elements": []
}